arr = []

n = int(input("Enter number of elements : "))

for i in range(0, n):
    ele = int(input())

    arr.append(ele)

print(arr)

def sortSquare(arr, n):
    for i in range(n):
        arr[i] = arr[i] * arr[i]
    arr.sort()

n = len(arr)

print("Before sort")
for i in range(n):
    print(arr[i], end=" ")

print("\n")

sortSquare(arr, n)

print("After sort")
for i in range(n):
    print(arr[i], end=" ")
